<!--
 Following configuration is necessary to use Java HBASE Client API.
 It desribes how to connect to HBASE!-->
<configuration>
    <property>
        <name>hbase.zookeeper.quorum</name>
        <value>localhost</value>
    </property>
    <property>
        <name>hbase.zookeeper.property.clientPort</name>
        <value>2181</value>
    </property>
</configuration>